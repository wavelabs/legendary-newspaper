<!DOCTYPE html>
<html>
  <head>
    <title>BaseCms</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <title>Diario Online</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Motive Mag Responsive web template, Bootstrap Web Templates, Flat Web Templates, Andriod Compatible web template,
    Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyErricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--/script-->
    <script type="text/javascript">
          jQuery(document).ready(function($) {
            $(".scroll").click(function(event){
              event.preventDefault();
              $('html,body').animate({scrollTop:$(this.hash).offset().top},900);
            });
          });
    </script>
  </head>
  <body>
<%= render 'shared/landing/header' %>
<%= yield %>
<%= render 'shared/landing/footer' %>

      <!--/start-copyright-section-->
        <div class="copyright">
              <p>&copy; 2017 Company <a href="http://wavelabs-studio.com/">Wavelabs</a> </p>
          </div>


        <!--start-smoth-scrolling-->
            <script type="text/javascript">
                  $(document).ready(function() {
                    $().UItoTop({ easingType: 'easeOutQuart' });
                  });
                </script>
    <a href="#home" id="toTop" class="scroll" style="display: block;"> <span id="toTopHover" style="opacity: 1;"> </span></a>
  </body>
</html>
