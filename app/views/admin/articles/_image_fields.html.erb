<div class="nested-fields">
  <div class="form-group field">
    <%= f.label 'Archivo de Imagen' %>
    <%= f.file_field :picture, class: 'form-control' , id: "imgInp" , data: {filename: f.object.picture_file_name} %>
    <div class="container">
      <div class="col-md-6">
        <img id= 'img-upload' />
      </div>
    </div>
  </div>

  <div class="form-group field">
    <%= f.label 'Descripcion de Imagen' %>
    <%= f.text_field :description, class: 'form-control' %>
  </div>

  <%= link_to_remove_association "Quitar Imagen", f, class: "btn btn-danger " %>
</div>
